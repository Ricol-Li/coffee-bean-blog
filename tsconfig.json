{
  "compilerOptions": {
    "target": "esnext",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions":true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "./typings"],
    "types":["./global"]
  },
  "include": ["./typings/*.d.ts", ".vitepress/**/*","global.d.ts"]
}